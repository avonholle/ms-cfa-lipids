@article{quiping_prescription_2014,
	title = {Prescription Cholesterol-lowering Medication Use in Adults Aged 40 and Over: United States, 2003–2012},
	abstract = {Cardiovascular disease ({CVD}) is the leading cause of morbidity and mortality
in the United States. Nearly one in three Americans dies of heart disease or
stroke (1). Elevated blood cholesterol is a major risk factor for {CVD}, and statin
therapy has been strongly associated with a reduced risk of atherosclerotic
{CVD} (2,3). The national cholesterol treatment guidelines outline the
importance of using cholesterol-lowering medications for the prevention of
coronary heart disease (4). Using National Health and Nutrition Examination
Survey ({NHANES}) data, this report evaluates recent trends in prescription
